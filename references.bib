  author = {Pasqualetti, F. and Dörfler, F. and Bullo, F.},
  date = {2013-11},
  pages = {2715--2729},
  keywords = {Biomedical monitoring,centralized attack detection monitor design,centralized attack identification monitor design,Control systems,Critical infrastructures,cyber-physical attacks,cyber-physical monitors,Cyber-physical systems,descriptor systems,distributed attack detection monitor design,distributed attack identification monitor design,distributed control,external malicious attacks,failure analysis,fault detection,fault diagnosis,geometric control,graph theory,industrial control processes,mathematical framework,Monitoring,networks,network theory (graphs),Noise,Power system dynamics,power systems,reliability theory,Security,Sensors,system theory,transportation networks},
}

